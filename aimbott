-- StarterGui.Settings.Open.OpenScript
local function C_2b()
local script = G2L["2b"];
	local TweenService = game:GetService("TweenService")
	local Lighting = game:GetService("Lighting")
	local Camera = workspace.CurrentCamera
	
	local button = script.Parent
	local frame = script.Parent.Parent:WaitForChild("MainFrame")
	local exitButton = frame:WaitForChild("Exit")
	local sound = script:WaitForChild("Sound")
	
	local Debounce = true
	
	local FOVIncrease = TweenService:Create(Camera, TweenInfo.new(0.2), {FieldOfView = 70})
	
	local function toggleGUI()
		if not Debounce then return end
		Debounce = false
	
		sound:Play()
	
		if frame.Visible then
			frame.Visible = false
			
		else
			frame.Visible = true
			FOVIncrease:Play()
		end
	
		task.wait(0.2)
		Debounce = true
	end
	
	button.MouseButton1Click:Connect(toggleGUI)
	exitButton.MouseButton1Click:Connect(toggleGUI)
	
end;
task.spawn(C_2b);

return G2L["1"], require;
