-- StarterGui.Settings.MainFrame.ScrollingFrame.Esp.LocalScript
local function C_c()
local script = G2L["c"];
	local button = script.Parent -- Bu toggle buton
	local espButton = script.Parent.Parent:WaitForChild("Esp") -- ESP butonuna erişim
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	local LocalPlayer = Players.LocalPlayer
	local isActive = false
	
	-- Ayarlar
	local settings = {
		showHealth = true,
		showDistance = true,
		showName = true,
		showTeam = true,
		maxDistance = 1000, -- Max görüntülenecek mesafe
		refreshRate = 0.1, -- Güncelleme hızı (saniye)
		healthBarLength = 30, -- Can barı uzunluğu
		healthBarThickness = 2, -- Can barı kalınlığı
		textSize = 14, -- Yazı boyutu
		textOffset = Vector2.new(0, 10) -- Yazı pozisyon offseti
	}
	
	-- ESP nesnelerini saklamak için
	local espCache = {}
	
	local function createESP(targetPlayer)
		if espCache[targetPlayer] then return end -- Zaten varsa tekrar oluşturma
	
		local espData = {
			highlight = nil,
			healthLabel = nil,
			distanceLabel = nil,
			nameLabel = nil,
			teamLabel = nil,
			connections = {}
		}
	
		espCache[targetPlayer] = espData
	
		local function updateCharacter(character)
			-- Önceki ESP nesnelerini temizle
			if espData.highlight then espData.highlight:Destroy() end
			if espData.healthLabel then espData.healthLabel:Destroy() end
			if espData.distanceLabel then espData.distanceLabel:Destroy() end
			if espData.nameLabel then espData.nameLabel:Destroy() end
			if espData.teamLabel then espData.teamLabel:Destroy() end
	
			-- Bağlantıları temizle
			for _, conn in ipairs(espData.connections) do
				conn:Disconnect()
			end
			espData.connections = {}
	
			if not character or not character:FindFirstChild("HumanoidRootPart") then return end
	
			local humanoid = character:FindFirstChildOfClass("Humanoid")
			if not humanoid then return end
	
			-- Highlight oluştur
			local highlight = Instance.new("Highlight")
			highlight.Name = "PlayerHighlight"
			highlight.Parent = character
			highlight.FillTransparency = 0.7
			highlight.OutlineTransparency = 0
			highlight.FillColor = targetPlayer.TeamColor.Color
			highlight.OutlineColor = Color3.new(1, 1, 1)
			highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
	
			-- BillboardGui'ler oluştur
			local function createLabel(text, color, positionOffset)
				local billboard = Instance.new("BillboardGui")
				billboard.Name = "ESPLabel"
				billboard.Adornee = character.Head or character.HumanoidRootPart
				billboard.Size = UDim2.new(0, 200, 0, 50)
				billboard.StudsOffset = positionOffset or Vector3.new(0, 0, 0)
				billboard.AlwaysOnTop = true
				billboard.MaxDistance = settings.maxDistance
				billboard.Parent = character
	
				local label = Instance.new("TextLabel")
				label.Name = "Label"
				label.Size = UDim2.new(1, 0, 0, settings.textSize)
				label.Position = UDim2.new(0.5, 0, 0, 0)
				label.AnchorPoint = Vector2.new(0.5, 0)
				label.BackgroundTransparency = 1
				label.Text = text
				label.TextColor3 = color or Color3.new(1, 1, 1)
				label.TextSize = settings.textSize
				label.TextStrokeTransparency = 0.5
				label.TextStrokeColor3 = Color3.new(0, 0, 0)
				label.Font = Enum.Font.GothamBold
				label.Parent = billboard
	
				return label
			end
	
			-- Can barı oluştur
			if settings.showHealth then
				local healthBillboard = Instance.new("BillboardGui")
				healthBillboard.Name = "HealthBar"
				healthBillboard.Adornee = character.Head or character.HumanoidRootPart
				healthBillboard.Size = UDim2.new(0, settings.healthBarLength, 0, settings.healthBarThickness + 4)
				healthBillboard.StudsOffset = Vector3.new(settings.textOffset.X, settings.textOffset.Y - 20, 0)
				healthBillboard.AlwaysOnTop = true
				healthBillboard.MaxDistance = settings.maxDistance
				healthBillboard.Parent = character
	
				local healthBackground = Instance.new("Frame")
				healthBackground.Name = "Background"
				healthBackground.Size = UDim2.new(1, 0, 1, 0)
				healthBackground.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
				healthBackground.BorderSizePixel = 0
				healthBackground.Parent = healthBillboard
	
				local healthBar = Instance.new("Frame")
				healthBar.Name = "Bar"
				healthBar.Size = UDim2.new(humanoid.Health / humanoid.MaxHealth, 0, 1, 0)
				healthBar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
				healthBar.BorderSizePixel = 0
				healthBar.Parent = healthBackground
	
				-- Can değişikliklerini takip et
				table.insert(espData.connections, humanoid.HealthChanged:Connect(function()
					local healthPercent = humanoid.Health / humanoid.MaxHealth
					healthBar.Size = UDim2.new(healthPercent, 0, 1, 0)
	
					-- Can durumuna göre renk değiştir
					if healthPercent > 0.5 then
						healthBar.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Yeşil
					elseif healthPercent > 0.2 then
						healthBar.BackgroundColor3 = Color3.fromRGB(255, 255, 0) -- Sarı
					else
						healthBar.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Kırmızı
					end
				end))
	
				espData.healthLabel = healthBillboard
			end
	
			-- Diğer bilgiler
			if settings.showName then
				espData.nameLabel = createLabel(targetPlayer.Name, targetPlayer.TeamColor.Color, 
					Vector3.new(settings.textOffset.X, settings.textOffset.Y, 0))
			end
	
			if settings.showDistance then
				espData.distanceLabel = createLabel("", Color3.new(1, 1, 1), 
					Vector3.new(settings.textOffset.X, settings.textOffset.Y + 15, 0))
			end
	
			if settings.showTeam then
				espData.teamLabel = createLabel("[ "..targetPlayer.Team.Name.." ]", targetPlayer.TeamColor.Color, 
					Vector3.new(settings.textOffset.X, settings.textOffset.Y + 30, 0))
			end
	
			espData.highlight = highlight
			espData.character = character
		end
	
		-- Karakter yüklendiğinde ESP'yi oluştur
		if targetPlayer.Character then
			updateCharacter(targetPlayer.Character)
		end
	
		-- Yeni karakterler için ESP'yi ekle
		table.insert(espData.connections, targetPlayer.CharacterAdded:Connect(updateCharacter))
	
		-- Oyuncu çıktığında temizlik yap
		table.insert(espData.connections, targetPlayer.AncestryChanged:Connect(function(_, parent)
			if not parent then
				if espData.highlight then espData.highlight:Destroy() end
				if espData.healthLabel then espData.healthLabel:Destroy() end
				if espData.distanceLabel then espData.distanceLabel:Destroy() end
				if espData.nameLabel then espData.nameLabel:Destroy() end
				if espData.teamLabel then espData.teamLabel:Destroy() end
				espCache[targetPlayer] = nil
			end
		end))
	end
	
	local function removeESP(targetPlayer)
		if not espCache[targetPlayer] then return end
	
		local espData = espCache[targetPlayer]
		if espData.highlight then espData.highlight:Destroy() end
		if espData.healthLabel then espData.healthLabel:Destroy() end
		if espData.distanceLabel then espData.distanceLabel:Destroy() end
		if espData.nameLabel then espData.nameLabel:Destroy() end
		if espData.teamLabel then espData.teamLabel:Destroy() end
	
		-- Bağlantıları kes
		for _, conn in ipairs(espData.connections) do
			conn:Disconnect()
		end
	
		espCache[targetPlayer] = nil
	end
	
	local function updateESPs()
		for player, espData in pairs(espCache) do
			if espData.distanceLabel and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local distance = (LocalPlayer.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
				espData.distanceLabel.TextLabel.Text = string.format("%.1f studs", distance)
			end
		end
	end
	
	local function addAllESPs()
		for _, player in ipairs(Players:GetPlayers()) do
			if player ~= LocalPlayer then
				createESP(player)
			end
		end
	end
	
	local function removeAllESPs()
		for player in pairs(espCache) do
			removeESP(player)
		end
	end
	
	-- Oyuncu eklendiğinde ESP ekle
	Players.PlayerAdded:Connect(function(player)
		if isActive then
			createESP(player)
		end
	end)
	
	-- Oyuncu çıktığında ESP'yi kaldır
	Players.PlayerRemoving:Connect(function(player)
		removeESP(player)
	end)
	
	-- Ana toggle fonksiyonu
	button.MouseButton1Click:Connect(function()
		isActive = not isActive
	
		if isActive then
			addAllESPs()
			espButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Yeşil
	
			-- ESP'leri periyodik olarak güncelle
			RunService:BindToRenderStep("ESPUpdate", Enum.RenderPriority.Last.Value, updateESPs)
		else
			removeAllESPs()
			espButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40) -- Orijinal gri ton
			RunService:UnbindFromRenderStep("ESPUpdate")
		end
	end)
	
	-- Oyun başladığında zaten var olan oyuncular için
	if isActive then
		addAllESPs()
		RunService:BindToRenderStep("ESPUpdate", Enum.RenderPriority.Last.Value, updateESPs)
		espButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
	end
end;
task.spawn(C_c);
-- StarterGui.Settings.MainFrame.ScrollingFrame.Destroy.LocalScript
local function C_f()
local script = G2L["f"];
	script.Parent.MouseButton1Click:Connect(function()
		-- GUI öğesini yok etmek için
		local gui = script.Parent:FindFirstAncestorOfClass("ScreenGui")  -- Butonun bağlı olduğu ScreenGui'yi bulur
		if gui then
			gui:Destroy()  -- GUI öğesini yok eder
		end
	end)
	
end;
task.spawn(C_f);
-- StarterGui.Settings.MainFrame.ScrollingFrame.Team.LocalScript
local function C_12()
local script = G2L["12"];
	local fov = 100 -- Çemberin boyutu
	local isActive = false -- Özellik başlangıçta kapalı
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local Cam = game.Workspace.CurrentCamera
	local LocalPlayer = Players.LocalPlayer
	
	-- FOV çemberi oluştur
	local FOVring = Drawing.new("Circle")
	FOVring.Visible = false
	FOVring.Thickness = 2
	FOVring.Color = Color3.fromRGB(255, 0, 0)
	FOVring.Filled = false
	FOVring.Radius = fov
	FOVring.Position = Cam.ViewportSize / 2
	
	-- Çemberin konumunu güncelle
	local function updateDrawings()
		FOVring.Position = Cam.ViewportSize / 2
	end
	
	-- En yakın oyuncuya bakış
	local function lookAt(target)
		local lookVector = (target - Cam.CFrame.Position).unit
		local newCFrame = CFrame.new(Cam.CFrame.Position, Cam.CFrame.Position + lookVector)
		Cam.CFrame = newCFrame
	end
	
	-- FOV içindeki en yakın görünən düşman oyuncuyu bul
	local function getClosestEnemyInFOV(trg_part)
		local nearest = nil
		local last = math.huge
		local playerMousePos = Cam.ViewportSize / 2
	
		for _, player in ipairs(Players:GetPlayers()) do
			if player ~= LocalPlayer and player.Team ~= LocalPlayer.Team then
				local character = player.Character
				local part = character and character:FindFirstChild(trg_part)
				if part then
					local ePos, onScreen = Cam:WorldToViewportPoint(part.Position)
					local distance = (Vector2.new(ePos.X, ePos.Y) - playerMousePos).Magnitude
	
					if onScreen and distance < fov and distance < last then
						-- Raycast (divar arxasındadırmı yoxlaması)
						local rayParams = RaycastParams.new()
						rayParams.FilterDescendantsInstances = {LocalPlayer.Character, workspace.Terrain}
						rayParams.FilterType = Enum.RaycastFilterType.Blacklist
	
						local result = workspace:Raycast(Cam.CFrame.Position, (part.Position - Cam.CFrame.Position).Unit * 500, rayParams)
	
						if result and result.Instance:IsDescendantOf(character) then
							nearest = player
							last = distance
						end
					end
				end
			end
		end
	
		return nearest
	end
	
	-- Özelliği etkinleştirme
	local function enableFeature()
		isActive = true
		FOVring.Visible = true
	
		RunService:BindToRenderStep("FOVUpdate", Enum.RenderPriority.Camera.Value, function()
			if not isActive then return end
			updateDrawings()
			local closest = getClosestEnemyInFOV("Head")
			if closest and closest.Character and closest.Character:FindFirstChild("Head") then
				lookAt(closest.Character.Head.Position)
			end
		end)
	end
	
	-- Özelliği devre dışı bırakma
	local function disableFeature()
		isActive = false
		FOVring.Visible = false
		RunService:UnbindFromRenderStep("FOVUpdate")
	end
	
	-- Butona tıklanıldığında toggle işlevi
	script.Parent.MouseButton1Click:Connect(function()
		if isActive then
			disableFeature()
		else
			enableFeature()
		end
	end)
	
end;
task.spawn(C_12);
-- StarterGui.Settings.MainFrame.ScrollingFrame.Aimbot.LocalScript
local function C_15()
local script = G2L["15"];
	local fov = 100 -- Çemberin boyutu
	local isActive = false -- Özellik başlangıçta kapalı
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local Cam = game.Workspace.CurrentCamera
	
	-- FOV çemberi oluştur
	local FOVring = Drawing.new("Circle")
	FOVring.Visible = false
	FOVring.Thickness = 2
	FOVring.Color = Color3.fromRGB(255, 0, 0) -- Mor renk
	FOVring.Filled = false
	FOVring.Radius = fov
	FOVring.Position = Cam.ViewportSize / 2
	
	-- Çemberin konumunu güncelle
	local function updateDrawings()
		FOVring.Position = Cam.ViewportSize / 2
	end
	
	-- En yakın oyuncuya bakış
	local function lookAt(target)
		local lookVector = (target - Cam.CFrame.Position).unit
		local newCFrame = CFrame.new(Cam.CFrame.Position, Cam.CFrame.Position + lookVector)
		Cam.CFrame = newCFrame
	end
	
	-- FOV içindeki en yakın oyuncuyu bul
	local function getClosestPlayerInFOV(trg_part)
		local nearest = nil
		local last = math.huge
		local playerMousePos = Cam.ViewportSize / 2
	
		for _, player in ipairs(Players:GetPlayers()) do
			if player ~= Players.LocalPlayer then
				local part = player.Character and player.Character:FindFirstChild(trg_part)
				if part then
					local ePos, isVisible = Cam:WorldToViewportPoint(part.Position)
					local distance = (Vector2.new(ePos.X, ePos.Y) - playerMousePos).Magnitude
	
					if distance < last and isVisible and distance < fov then
						last = distance
						nearest = player
					end
				end
			end
		end
	
		return nearest
	end
	
	-- Özelliği etkinleştirme
	local function enableFeature()
		isActive = true
		FOVring.Visible = true
	
		RunService:BindToRenderStep("FOVUpdate", Enum.RenderPriority.Camera.Value, function()
			if not isActive then return end
			updateDrawings()
			local closest = getClosestPlayerInFOV("Head")
			if closest and closest.Character:FindFirstChild("Head") then
				lookAt(closest.Character.Head.Position)
			end
		end)
	end
	
	-- Özelliği devre dışı bırakma
	local function disableFeature()
		isActive = false
		FOVring.Visible = false
		RunService:UnbindFromRenderStep("FOVUpdate")
	end
	
	-- Butona tıklanıldığında toggle işlevi
	script.Parent.MouseButton1Click:Connect(function()
		if isActive then
			disableFeature() -- Eğer aktifse, kapat
		else
			enableFeature() -- Eğer kapalıysa, aç
		end
	end)
	
end;
task.spawn(C_15);
