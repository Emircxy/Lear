                        else
                            if platform then
                                platform:Destroy()
                                platform = nil
                            end
                        end
                        aw.CFrame = ax.CFrame * CFrame.Angles(0, math.pi, 0) * CFrame.new(0, 0, 0)
                        task.wait(.15)
                        aw.CFrame = ax.CFrame * CFrame.Angles(0, math.pi, 0) * CFrame.new(0, 0, 1)
                        task.wait(.15)
                        aw.CFrame = ax.CFrame * CFrame.Angles(0, math.pi, 0) * CFrame.new(0, 0, 2)
                    end
                end
            end,
}) 

